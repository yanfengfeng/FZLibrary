<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.fengzi.ui.permissions.PermissionsAc">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="0dp">

        <Button
            android:id="@+id/btn_andPermission"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="addPermissions(第三方)" />

        <Button
            android:id="@+id/btn_permissionsDispatcher"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/btn_camer"
            android:text="permissionsDispatcher(第三方)" />

        <Button
            android:id="@+id/btn_myslef"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/btn_photo"
            android:text="授权使用(myslef)" />


    </LinearLayout>


</android.support.constraint.ConstraintLayout>
